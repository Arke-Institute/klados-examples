{
  "label": "Stamp Worker",
  "description": "Example worker that stamps entities with metadata",
  "endpoint": "https://stamp-worker.YOUR-SUBDOMAIN.workers.dev",
  "actions_required": ["entity:view", "entity:update"],
  "accepts": {
    "types": ["*"],
    "cardinality": "one"
  },
  "produces": {
    "types": ["*"],
    "cardinality": "one"
  }
}
